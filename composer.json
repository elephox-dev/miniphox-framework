{
  "name": "elephox/miniphox-framework",
  "description": "A minimal API framework based on Elephox.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ricardo Boss",
      "email": "contact@ricardoboss.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "elephox/framework": "dev-develop",
    "react/http": "^1.8",
    "ricardoboss/php-console-logger": "^2.4",
    "psr/http-server-handler": "^1.0",
    "fruitcake/php-cors": "dev-feature/psr-7-rewrite",
    "psr/http-server-middleware": "^1.0"
  },
  "provide": {
    "psr/http-server-handler-implementation": "1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Elephox\\Miniphox\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/elephox-dev/php-cors-psr-7"
    }
  ],
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
